<%
[result|tail] = String.split(@result,"\n")
message = Enum.join(tail,"\n")
%>
<div>
<h1><%="stage #{@area}-#{@stage} "<>result%></h1>
<p><%=message%></p>
<table>
<tr>
<td width="5%">Miss:</td>
<td><%= @error %></td>
</tr>
misstypes: <%= @misstypes %>
</table>
<%= button "ステージ選択に戻る", to: "/game/stage/"<>@area, method: :get%>
<%= button "Try Again", to: "/game/main/"<>@area<>"/"<>@stage %>
</div>
